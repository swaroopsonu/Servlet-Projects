<?xml version="1.0" encoding="UTF-8"?>
<web-app>

<servlet>
<servlet-name>BeanServlet</servlet-name>
<servlet-class>test.BeanServlet</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>BeanServlet</servlet-name>
<url-pattern>/bean</url-pattern>
</servlet-mapping>

